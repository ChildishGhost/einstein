{
	"name": "einstein",
	"version": "0.0.1",
	"main": "./dist/main/main.js",
	"scripts": {
		"postinstall": "patch-package",
		"build": "npm run build:webpack && npm run build:electron",
		"build:webpack": "webpack",
		"build:electron": ".bin/build-electron.sh",
		"clean": "rm -rf node_modules && rm -rf dist/{electron,main,node,plugins,renderer}",
		"lint": "eslint --ext .ts,.js,.vue src/ plugins/",
		"lint:fix": "npm run lint -- --fix",
		"format": "prettier -w src/ plugins/ && eslint --ext .ts,.js,.vue --fix src/ plugins/",
		"run": "npm run build:webpack && electron .",
		"run:without-build": "electron .",
		"watch": "webpack --watch"
	},
	"devDependencies": {
		"@babel/core": "^7.25.2",
		"@types/node": "^16.11.19",
		"@typescript-eslint/eslint-plugin": "^7.17.0",
		"@typescript-eslint/parser": "^7.17.0",
		"@vue/compiler-sfc": "^3.4.38",
		"babel-loader": "^9.1.3",
		"babel-plugin-module-resolver": "^5.0.2",
		"clean-webpack-plugin": "^4.0.0",
		"create-file-webpack": "^1.0.2",
		"css-loader": "^7.1.2",
		"electron": "^32.0.1",
		"electron-debug": "^4.0.1",
		"eslint": "^8.57.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-tsdoc": "^0.3.0",
		"eslint-plugin-vue": "^9.27.0",
		"eslint-webpack-plugin": "^4.2.0",
		"html-webpack-plugin": "^5.6.0",
		"mini-css-extract-plugin": "^2.9.1",
		"patch-package": "^8.0.0",
		"prettier": "^3.3.3",
		"prettier-eslint": "^16.3.0",
		"sass": "^1.77.8",
		"sass-loader": "^16.0.0",
		"ts-loader": "^9.5.1",
		"typescript": "^5.5.4",
		"vue-eslint-parser": "^9.4.3",
		"vue-loader": "^17.4.2",
		"webpack": "^5.94.0",
		"webpack-cli": "^5.1.4",
		"webpack-dev-server": "^5.0.4",
		"webpack-hot-middleware": "^2.26.1",
		"webpack-merge": "^6.0.1"
	},
	"dependencies": {
		"comment-json": "^4.2.5",
		"fuse.js": "^7.0.0",
		"source-map-support": "^0.5.21",
		"vm2": "^3.9.19",
		"vue": "^3.4.38"
	}
}
