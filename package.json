{
	"name": "einstein",
	"version": "0.0.1",
	"main": "./dist/main/main.js",
	"scripts": {
		"postinstall": "patch-package",
		"build": "npm run build:webpack && npm run build:electron",
		"build:webpack": "webpack",
		"build:electron": ".bin/build-electron.sh",
		"clean": "rm -rf node_modules && rm -rf dist/{electron,main,node,plugins,renderer}",
		"lint": "eslint --ext .ts,.js,.vue src/ plugins/",
		"lint:fix": "npm run lint -- --fix",
		"format": "prettier -w src/ plugins/ && eslint --ext .ts,.js,.vue --fix src/ plugins/",
		"run": "npm run build:webpack && electron .",
		"run:without-build": "electron .",
		"watch": "webpack --watch"
	},
	"devDependencies": {
		"@babel/core": "^7.22.9",
		"@types/node": "^16.11.19",
		"@typescript-eslint/eslint-plugin": "^5.59.11",
		"@typescript-eslint/parser": "^5.59.8",
		"@vue/compiler-sfc": "^3.2.45",
		"babel-loader": "^9.1.3",
		"babel-plugin-module-resolver": "^5.0.0",
		"clean-webpack-plugin": "^4.0.0",
		"create-file-webpack": "^1.0.2",
		"css-loader": "^6.8.1",
		"electron": "^25.3.2",
		"electron-debug": "^3.2.0",
		"eslint": "^8.47.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.9.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-import": "^2.28.0",
		"eslint-plugin-tsdoc": "^0.2.17",
		"eslint-plugin-vue": "^9.17.0",
		"eslint-webpack-plugin": "^4.0.1",
		"html-webpack-plugin": "^5.5.1",
		"mini-css-extract-plugin": "^2.7.6",
		"patch-package": "^8.0.0",
		"prettier": "^3.0.0",
		"prettier-eslint": "^15.0.1",
		"sass": "^1.64.2",
		"sass-loader": "^13.3.2",
		"ts-loader": "^9.4.4",
		"typescript": "^5.1.6",
		"vue-eslint-parser": "^9.3.1",
		"vue-loader": "^17.2.2",
		"webpack": "^5.87.0",
		"webpack-cli": "^5.1.1",
		"webpack-dev-server": "^4.15.1",
		"webpack-hot-middleware": "^2.25.4",
		"webpack-merge": "^5.9.0"
	},
	"dependencies": {
		"comment-json": "^4.2.3",
		"fuse.js": "^6.6.2",
		"source-map-support": "^0.5.21",
		"vm2": "^3.9.19",
		"vue": "^3.3.4"
	}
}
