{
	"name": "einstein",
	"version": "0.0.1",
	"main": "./dist/main/main.js",
	"scripts": {
		"postinstall": "patch-package",
		"build": "npm run build:webpack && npm run build:electron",
		"build:webpack": "webpack",
		"build:electron": ".bin/build-electron.sh",
		"clean": "rm -rf node_modules && rm -rf dist/{electron,main,node,plugins,renderer}",
		"lint": "eslint --ext .ts,.js,.vue src/ plugins/",
		"lint:fix": "npm run lint -- --fix",
		"format": "prettier -w src/ plugins/ && eslint --ext .ts,.js,.vue --fix src/ plugins/",
		"run": "npm run build:webpack && electron .",
		"run:without-build": "electron .",
		"watch": "webpack --watch"
	},
	"devDependencies": {
		"@babel/core": "^7.18.9",
		"@babel/preset-env": "^7.18.9",
		"@types/node": "^16.11.19",
		"@typescript-eslint/eslint-plugin": "^5.32.0",
		"@typescript-eslint/parser": "^5.31.0",
		"@vue/compiler-sfc": "^3.2.37",
		"babel-loader": "^8.2.5",
		"babel-plugin-module-resolver": "^4.1.0",
		"clean-webpack-plugin": "^4.0.0",
		"create-file-webpack": "^1.0.2",
		"css-loader": "^6.7.1",
		"electron": "^20.0.1",
		"electron-debug": "^3.2.0",
		"eslint": "^8.21.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-import-resolver-typescript": "^3.3.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-tsdoc": "^0.2.16",
		"eslint-plugin-vue": "^9.3.0",
		"eslint-webpack-plugin": "^3.2.0",
		"html-webpack-plugin": "^5.5.0",
		"mini-css-extract-plugin": "^2.6.1",
		"patch-package": "^6.4.7",
		"prettier": "^2.7.1",
		"prettier-eslint": "^15.0.1",
		"sass": "^1.54.3",
		"sass-loader": "^13.0.2",
		"ts-loader": "^9.3.1",
		"typescript": "^4.7.4",
		"vue-eslint-parser": "^9.0.3",
		"vue-loader": "^17.0.0",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0",
		"webpack-dev-server": "^4.9.3",
		"webpack-hot-middleware": "^2.25.1",
		"webpack-merge": "^5.8.0"
	},
	"dependencies": {
		"comment-json": "^4.2.2",
		"core-js": "^3.24.1",
		"fuse.js": "^6.6.2",
		"source-map-support": "^0.5.21",
		"vm2": "^3.9.10",
		"vue": "^3.2.37"
	}
}
